{
  "family": "quest-task",
  "containerDefinitions": [
    {
      "name": "quest-container",
      "image": "quest-ecr:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::417297593891:role/quest-execution-task-role",
  "requiresCompatibilities": [
    "FARGATE"
  ]
}